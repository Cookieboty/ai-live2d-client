{
  "name": "@ig-live/renderer",
  "version": "1.0.0",
  "description": "基于React的前端UI",
  "scripts": {
    "dev": "vite",
    "prebuild": "npm run copy-assets",
    "build": "tsc && vite build",
    "copy-assets": "mkdir -p dist/assets/live2d-models && cp -R public/assets/* dist/assets/ && mkdir -p dist/assets/live2d-widget",
    "clean": "rimraf dist",
    "test": "vitest run"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@ig-live/types": "workspace:*",
    "@types/node": "^16.18.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "copyfiles": "^2.4.1",
    "rimraf": "^5.0.0",
    "terser": "^5.39.2",
    "typescript": "^4.9.0",
    "vite": "^4.3.9",
    "vitest": "^0.32.0"
  }
}